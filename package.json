        "del": "rm -r target/*",
        "build": "yarn release && yarn html && yarn serve"
  },
  "devDependencies": {
    "shadow-cljs": "^2.19.2",
    "http-server": "^0.12.3"
  },
  "dependencies": {
    "plotly.js-dist-min": "^2.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
