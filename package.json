  "devDependencies": {
    "gh-pages": "^5.0.0",
    "http-server": "^0.12.3",
    "shadow-cljs": "^2.19.2"
  },
  "dependencies": {
    "plotly.js-dist-min": "^2.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
