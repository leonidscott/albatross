{
  "name": "albatross",
  "version": "0.1.0",
  "main": "index.js",
  "description": "Explore NACA 4 foils, Thin Aerofoil Theory, and Evolutionary Computation",
  "scripts": {
    "watch": "shadow-cljs watch app",
    "compile": "shadow-cljs compile app",
